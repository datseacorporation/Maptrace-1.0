<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
android:background="@color/colorWhite"
    tools:context="com.datseacorporation.maptrace.Log">

    <WebView
        android:id="@+id/newWebapp"
        android:layout_width="match_parent"
        android:layout_height="450sp"
        android:layout_marginTop="110sp"
        >
    </WebView>

    <Button
        android:id="@+id/refreshmapbtn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewLogbtn"
        android:text="Refresh Map" />

    <Button
        android:id="@+id/viewLogbtn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="Show Logs" />
</RelativeLayout>





